{
    "name": "log1x/rom-cli",
    "description": "Simple CLI tool to get Ragnarok Mobile exchange prices from ROM Exchange.",
    "keywords": ["cli", "console", "ragnarok", "rom"],
    "homepage": "https://github.com/log1x/rom-cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/log1x/rom-cli/issues",
        "source": "https://github.com/log1x/rom-cli"
    },
    "authors": [
        {
            "name": "Brandon Nifong",
            "email": "brandon@tendency.me"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-zero/framework": "5.8.*",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["rom-cli"]
}
